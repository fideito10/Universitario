# Archivo de ejemplo para configurar secrets en Streamlit Cloud
# NO subir este archivo a GitHub - solo usar como referencia

[gcp_service_account]
type = "service_account"
project_id = "tu-project-id-aqui"
private_key_id = "tu-private-key-id-aqui"
private_key = "-----BEGIN PRIVATE KEY-----\ntu-private-key-completa-aqui\n-----END PRIVATE KEY-----\n"
client_email = "tu-service-account@tu-project.iam.gserviceaccount.com"
client_id = "tu-client-id-aqui"
auth_uri = "https://accounts.google.com/o/oauth2/auth"
token_uri = "https://oauth2.googleapis.com/token"
auth_provider_x509_cert_url = "https://www.googleapis.com/oauth2/v1/certs"
client_x509_cert_url = "https://www.googleapis.com/service/v1/metadata/x509/tu-service-account%40tu-project.iam.gserviceaccount.com"

# Instrucciones:
# 1. Copia el contenido de tu archivo service_account.json
# 2. Reemplaza cada campo con los valores correspondientes
# 3. Pega este contenido en la secci√≥n "Secrets" de tu app en Streamlit Cloud
# 4. NO incluyas las comillas exteriores para cada valor